{
  "name": "heythere-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "android": "react-native run-android --appId=com.co188.heythere.debug",
    "ios": "react-native run-ios --simulator='iPhone 14 Pro'",
    "ios-list": "xcrun simctl list devices",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "pod": "npx pod-install ios",
    "font": "npx react-native-asset --ios-assets && echo 'Android fonts should manually addded with xml! (https://stackoverflow.com/a/70247374/3535760)'",
    "install-a": "android/gradlew -p android/app installRelease",
    "bundle-a": "android/gradlew -p android/app bundleRelease && cp android/app/build/outputs/bundle/release/app-release.aab $HOME/Desktop/$(date '+%y%m%d_%H%M%S').aab",
    "assemble-a": "android/gradlew -p android/app assembleRelease && cp android/app/build/outputs/apk/release/app-release.apk $HOME/Desktop/$(date '+%y%m%d_%H%M%S').apk",
    "cp-a": "appcenter codepush release-react -a 188-Corporation/heymatch-client-android -d Production",
    "cp-i": "appcenter codepush release-react -a 188-Corporation/heymatch-client-ios -d Production",
    "ver": "scripts/increment-version.sh"
  },
